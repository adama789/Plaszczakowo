@page "/carrier_assignment"
@using Problem.CarrierAssignment;
@inherits ProblemComponentBase<CarrierAssignmentInputData, CarrierAssignmentOutput, Drawer.GraphDrawer.GraphData>

@if (Executor is not null)
{
    <GraphVisualizer Snapshots="Executor.GetSnapshots()"/>
}


@code {

    protected override void InitializeResolver()
    {
        Resolver = new CarrierAssignmentResolver();
    }

    protected override void InitializeFirstSnapshotCreator(CarrierAssignmentInputData inputData)
    {
        FirstSnapshotCreator = new FirstCarrierAssignmentSnapshotCreator(inputData);
    }
}